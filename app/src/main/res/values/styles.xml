    <style name="CursorTheme" parent="AppTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/appBackGroundColor</item>
        <item name="colorPrimaryDark">@color/appBackGroundColor</item>
        <item name="colorAccent">@color/white</item>
    </style>

</resources>
